{
  "name": "quill Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1a2e",
    "editor.foreground": "#eaeaea",
    "editorLineNumber.foreground": "#4a4a6a",
    "editorLineNumber.activeForeground": "#9333ea",
    "editor.selectionBackground": "#9333ea40",
    "editor.lineHighlightBackground": "#16213e30",
    "editorCursor.foreground": "#ec4899",
    "editorWhitespace.foreground": "#4a4a6a40"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#6b7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Control Keywords (if, then, else, end, while, for)",
      "scope": "keyword.control.quill",
      "settings": {
        "foreground": "#9333ea",
        "fontStyle": "bold"
      }
    },
    {
      "name": "quill Commands (say, ask, choice, set)",
      "scope": "keyword.other.quill.command",
      "settings": {
        "foreground": "#ec4899",
        "fontStyle": "bold"
      }
    },
    {
      "name": "quill Other Keywords (function, import)",
      "scope": "keyword.other.quill",
      "settings": {
        "foreground": "#7c3aed",
        "fontStyle": ""
      }
    },
    {
      "name": "Logical Operators (and, or, not, is)",
      "scope": "keyword.operator.logical.quill",
      "settings": {
        "foreground": "#f59e0b",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": "string.quoted",
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#06b6d4",
        "fontStyle": ""
      }
    },
    {
      "name": "Operators (+, -, *, /, ==, !=)",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "Variables",
      "scope": "variable.other.quill",
      "settings": {
        "foreground": "#fbbf24",
        "fontStyle": ""
      }
    },
    {
      "name": "Constants (true, false, null)",
      "scope": "constant.language",
      "settings": {
        "foreground": "#8b5cf6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": "entity.name.function.builtin",
      "settings": {
        "foreground": "#a78bfa",
        "fontStyle": ""
      }
    }
  ]
}
